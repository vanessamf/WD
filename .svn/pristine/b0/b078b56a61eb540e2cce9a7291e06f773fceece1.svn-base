	<body>
		<div class="gc-container sk-container">
			<div class="sk-box">
				<div class="top-header">
					<div class="back-box">
						<a href="homePage.html"></a>
					</div>
					<div class="tit">快速收款</div>
				</div>
				<div id="sk_wrapper" class="sk m-scroll-wrap">
					<main>
						<div class="sum-item">
							<div class="sum">
								<span>收款金额</span>
								<!--<textarea name="textarea" rows="1" cols="" placeholder="收款金额"></textarea>-->
								<div class="money">
									<span>￥</span>
									<input type="text" name="" id="" value="" placeholder="" />
								</div>
							</div>
							<div class="remark">
								<span>添加备注</span>
								<textarea name="textarea" rows="2" cols="" placeholder=""></textarea>
								<!--<input type="text" name="" id="" value="" placeholder="" />-->
							</div>
						</div>
						<div class="vip">
							<span>
								会员
								<em>（非必填）</em>
							</span>
							<input type="text" name="" id="" value="" placeholder="会员手机号/会员卡号" />
						</div>
						<!--<input type="submit" name="" id="" value="去收款" class="btn" />-->
						<div class="keyboard-wrapper">
							<ul class="num">
								<li class="hair-top hair-right">7</li>
								<li class="hair-top hair-right">8</li>
								<li class="hair-top hair-right">9</li>
								<li class="hair-top hair-right">4</li>
								<li class="hair-top hair-right">5</li>
								<li class="hair-top hair-right">6</li>
								<li class="hair-top hair-right">1</li>
								<li class="hair-top hair-right">2</li>
								<li class="hair-top hair-right">3</li>
								<li class="hair-top hair-right">0</li>
								<li class="hair-top hair-right">.</li>
								<li class="hair-top hair-right">00</li>
							</ul>
							<ul class="char">
								<li class="hair-top icon-delete"><i></i></li>
								<li class="sk-btn hair-top">
									<a href="payType.html">去收款</a>
								</li>
							</ul>
						</div>
					</main>
				</div>
			</div>
		</div>

