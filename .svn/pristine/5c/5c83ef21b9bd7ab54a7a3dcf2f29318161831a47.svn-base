/*1px*/

.hair-top:before,
.hair-left:before,
.hair-bottom:after,
.hair-right:after {
	content: '';
	display: block;
	position: absolute;
	background-color: #aaa;
}

.hair-top:before {
	width: 100%;
	height: 1px;
	top: 0;
	left: 0;
	/*transform: scaleY(.5);*/
}

.hair-left:before {
	height: 100%;
	width: 1px;
	top: 0;
	left: 0;
	/*transform: scaleX(.5);*/
}

.hair-bottom:after {
	width: 100%;
	height: 1px;
	bottom: 0;
	left: 0;
	/*transform: scaleY(.5);*/
}

.hair-right:after {
	height: 100%;
	width: 1px;
	top: 0;
	right: 0;
	/*transform: scaleX(.5);*/
}


