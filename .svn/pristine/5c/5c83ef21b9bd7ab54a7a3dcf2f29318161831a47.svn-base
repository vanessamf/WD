	left: 0;
	width: 100%;
	height: 100%;
	z-index: 1000;
	background: #fff;
}

.loader-wrapper.ds-none {
	display: none
}

.loader:before {
	content: "";
	position: absolute;
	top: 6px;
	left: 6px;
	right: 6px;
	bottom: 6px;
	border-radius: 50%;
	border: 3px solid transparent;
	border-top-color: #b3a47d;
	-webkit-animation: spin 3s linear infinite;
	/* Chrome, Opera 15+, Safari 5+ */
	animation: spin 3s linear infinite;
	/* Chrome, Firefox 16+, IE 10+, Opera */
}

.loader {
	display: block;
	position: relative;
	left: 50%;
	top: 50%;
	width: 2.4rem;
	height: 2.4rem;
	margin: -1.4rem 0 0 -1.2rem;
	border-radius: 50%;
	border: 3px solid transparent;
	border-top-color: #fccc0d;
	-webkit-animation: spin 2s linear infinite;
	/* Chrome, Opera 15+, Safari 5+ */
	animation: spin 2s linear infinite;
	/* Chrome, Firefox 16+, IE 10+, Opera */
	z-index: 1001;
}

.loader:after {
	content: "";
	position: absolute;
	top: 16px;
	left: 16px;
	right: 16px;
	bottom: 16px;
	border-radius: 50%;
	border: 3px solid transparent;
	border-top-color: #a4a4a4;
	-webkit-animation: spin 1.6s linear infinite;
	/* Chrome, Opera 15+, Safari 5+ */
	animation: spin 1.6s linear infinite;
	/* Chrome, Firefox 16+, IE 10+, Opera */
}

@-webkit-keyframes spin {
	0% {
		-webkit-transform: rotate(0deg);
		/* Chrome, Opera 15+, Safari 3.1+ */
		-ms-transform: rotate(0deg);
		/* IE 9 */
		transform: rotate(0deg);
		/* Firefox 16+, IE 10+, Opera */
	}
	100% {
		-webkit-transform: rotate(360deg);
		/* Chrome, Opera 15+, Safari 3.1+ */
		-ms-transform: rotate(360deg);
		/* IE 9 */
		transform: rotate(360deg);
		/* Firefox 16+, IE 10+, Opera */
	}
}

@keyframes spin {
	0% {
		-webkit-transform: rotate(0deg);
		-ms-transform: rotate(0deg);
		transform: rotate(0deg);
	}
	100% {
		-webkit-transform: rotate(360deg);
		/* Chrome, Opera 15+, Safari 3.1+ */
		-ms-transform: rotate(360deg);
		/* IE 9 */
		transform: rotate(360deg);
		/* Firefox 16+, IE 10+, Opera */
	}
}

@-webkit-keyframes changeBg {
	0% {
		background: #f1d77a;
	}
	62% {
		background: #f1e093;
	}
	70% {
		background: rgba(241, 215, 134, 0.9)
	}
	100% {
		background: #f1cc53;
	}
}

@keyframes changeBg {
	0% {
		background: #f1d77a;
	}
	62% {
		background: #f1e093;
	}
	70% {
		background: rgba(241, 215, 134, 0.9)
	}
	100% {
